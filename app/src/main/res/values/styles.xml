<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="header_xlarge">
        <item name="android:textSize">32sp</item>
        <item name="android:lineSpacingExtra"> 40sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> -1 </item>
        
    </style>

    <style name="header_large">
        <item name="android:textSize">24sp</item>
        <item name="android:lineSpacingExtra">32sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> -1 </item>
        
    </style>

    <style name="header_medium">
        <item name="android:textSize">20sp</item>
        <item name="android:lineSpacingExtra">26sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> -1 </item>
        
    </style>

    <style name="header_small">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">22sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> -1 </item>
        
    </style>

    <style name="body">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">22sp </item>
        <item name="android:textStyle"> normal </item>
        
    </style>

    <style name="body_bold">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">22sp </item>
        <item name="android:textStyle"> bold </item>
        
    </style>

    <style name="call_out">
        <item name="android:textSize">13sp</item>
        <item name="android:lineSpacingExtra">16sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> 8 </item>
        
    </style>

    <style name="label">
        <item name="android:textSize">13sp</item>
        <item name="android:lineSpacingExtra">16sp </item>
        <item name="android:textStyle"> bold </item>
        
    </style>

    <style name="foot_note">
        <item name="android:textSize">13sp</item>
        <item name="android:lineSpacingExtra">16sp </item>
        <item name="android:textStyle"> bold </item>
        
    </style>

    <style name="tab">
        <item name="android:textSize">11sp</item>
        <item name="android:lineSpacingExtra">14sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> 1 </item>
        
    </style>

    <style name="note">
        <item name="android:textSize">11sp</item>
        <item name="android:lineSpacingExtra">14sp </item>
        <item name="android:textStyle"> bold </item>
        <item name="android:letterSpacing"> 2 </item>

    </style>

    <style name="strikehrough">
        <item name="android:textSize">11sp</item>
        <item name="android:foreground">@drawable/strikethrough</item>
        <item name="android:lineSpacingExtra">14sp </item>
        <item name="android:letterSpacing"> 2 </item>
        
    </style>









</resources>